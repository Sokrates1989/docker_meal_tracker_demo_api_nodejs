{
    "source": {
        "include": ["src", "index.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "template": "node_modules/jsdoc/templates/default" 
    },
    "tags": {
        "allowUnknownTags": true
    }
}
  